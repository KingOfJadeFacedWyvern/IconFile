<script type="text/javascript">
(function(){
	$id('chgmobile').onfocus=function(){
		if(this.value=="请输入手机号"){
			this.value='';
			this.style.color='#000';
		};
	};
	$id('chgmobile').onblur=function(){
		if(this.value==""){
			this.value='请输入手机号';
			this.style.color='#9d9b9e';
		}
	};
	$id('mobileAutoPrix').onchange=function(){
		var v=this.value,
			fix=v>10000?1:2;
		$id('mobileAutoPrixShow').innerHTML=((v*0.983)/100).toFixed(fix)+"-"+((v*0.994)/100).toFixed(fix);
	};
	$id('mobileAutoBtn').onclick=function(){
		var c=$id("chgmobile"),		
			e=$id("mobileAutoPrix"),
			cVal=c.value,
			eVal=e.value,
			para="tid=01001001&anonyuser=1&uin="+cVal+"&chgmobile="+cVal+"&amount="+eVal+"&chargeway=1&bankid=0&attach=paipai&version=1&PTAG=30053.20.8";
		
		if(cVal==""){
			alert("请输入手机号码。");
			c.focus();
			return;
		}
	
		if(!$isMobile(cVal)){
			alert("您输入的手机号码不正确，请重新输入！");
			c.focus();
			return;
		}
		
		if(eVal=="30000"&&$isMobile(cVal)=='联通'){
			alert("联通号码暂不支持300元充值！");
			e.focus();
			return;
		}
		window.open("http://auction.paipai.com/cgi-bin/vb2c_fixup/mobilesubmit?"+para);	
	};
	function $id(e){
		return document.getElementById(e);
	};
	function $isMobile(v){
	/*
	　　中国移动号段 1340-1348 135 136 137 138 139 150 151 152 157 158 159 187 188 147
	　　中国联通号段 130 131 132 155 156 185 186 145
	　　中国电信号段 133 1349 153 180 189
	*/	
		var cm="134,135,136,137,138,139,150,151,152,157,158,159,187,188,147",
			cu="130,131,132,155,156,185,186,145",
			ct="133,153,180,189",
			h1=v.substring(0,3),
			h2=v.substring(0,4),
			v=(/^1\d{10}$/).test(v)?(cu.indexOf(h1)>=0?"联通":(ct.indexOf(h1)>=0?"电信":(h2=="1349"?"电信":(cm.indexOf(h1)>=0?"移动":false)))):false;
		//首先找是否联通，然后查找是否电信，然后在移动中查找‘1349’为电信，最后在移动中查找
		return v; 
	};
})();
</script><!--[if !IE]>|xGv00|3dbe79955b8c7e6065096d4af22f273e<![endif]-->
