<script type="text/javascript">
var n=0,autoStart,showsPic,m=0;
function sp_focusImg(){
	if(document.getElementById("f_bn")){
		showsPic = document.getElementById("f_bn");
	}
	if(showsPic){
		m=showsPic.getElementsByTagName("a").length;
	}
}
function Mea(value){
	n=value;
	setBg(value);
	plays(value);
	cons(value);
}
function setBg(value){
	for(var i=0;i<m;i++)
    if(value==i){
		showsPic.getElementsByTagName("a")[i].className='f1';
	} 
	else{	
		showsPic.getElementsByTagName("a")[i].className='f2';
	}  
} 
function plays(value){
	try{
		with (f_img){
			filters[0].Apply();
			for(i=0;i<m;i++)
				i==value?children[i].style.display="block":children[i].style.display="none"; 
			filters[0].play(); 		
		}
	}
	catch(e){
		if(document.getElementById("f_img")){
			var d = document.getElementById("f_img").getElementsByTagName("div");
		}
		for(i=0;i<m;i++)
			i==value?d[i].className="nodis":d[i].className="dis"; 
	}
}
function cons(value)
{
    try{
		with (f_text){

			for(i=0;i<m;i++)i==value?children[i].className="nodis":children[i].className="dis"; 
 		
		}
	}
	catch(e){
		var d;
		if(document.getElementById("f_text")) { 
			d=document.getElementById("f_text").getElementsByTagName("div");
		};
		if(d){
			for(i=0;i<m;i++){i==value?d[i].className="nodis":d[i].className="dis";} 
		}
	}
}
function setAuto(){
	autoStart=setInterval("auto(n)", 5000);
}
function auto(){ 
	n++;
	if(n>=m)n=0;
	Mea(n);
} 
function sub(){
	n--;
	if(n<0)n=m-1;
	Mea(n);
} 
function clearAuto(){ 
    try{
	    clearInterval(autoStart)
	}
	catch(err){}
}
clearAuto();
setAuto();

window.onload = Init;
var _lastID = 0;
function Init(){
	sp_focusImg();
	//城市菜单
	var _hoveCitys = QQI.bind(QQI.getById("city"),QQI.hoveCitys);
	QQI.addEvent(QQI.getById("city"),'mouseover',_hoveCitys);
	var _outCitys = QQI.bind(QQI.getById("city"),QQI.outCitys);
	QQI.addEvent(QQI.getById("city"),'mouseout',_outCitys);
	var _hoveCitysMenu = QQI.bind(QQI.getById("citys_menu"),QQI.hoveCitysMenu);
	QQI.addEvent(QQI.getById("citys_menu"),'mouseover',_hoveCitysMenu);
	var _outCitysMenu = QQI.bind(QQI.getById("citys_menu"),QQI.outCitysMenu);
	QQI.addEvent(QQI.getById("citys_menu"),'mouseout',_outCitysMenu);	
}
</script>
