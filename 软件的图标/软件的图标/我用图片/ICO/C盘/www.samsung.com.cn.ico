						iframeUrl += "&group=" + arg1 + "&type=" + arg2 + "&model_cd=" + arg3 + "&display_name=" + escape(arg4);
						if (arg5 == "") {
							iframeUrl += "&pagelink=" + encodeURIComponent(document.location);
							iframeControl.src = iframeUrl;
							showTellaFriendLayer("on", "tellaFriend");
						} else {
							iframeUrl += "&pagelink=" + encodeURIComponent(arg5);
							dockIframeControl.src = iframeUrl;
							viewEmailToFriend("on");
						}
					}
				}
				
				function showTellaFriendLayer(value,targetid){
					if (value=='on') {
						document.getElementById('visualsearch_transparency').style.display='block';
						if(document.getElementById(targetid)) document.getElementById(targetid).style.display='block';
						document.getElementById("wrap").setAttribute("whereEmail",'content');
					} else {
						if(document.getElementById(targetid)) document.getElementById(targetid).style.display='none';
						document.getElementById('visualsearch_transparency').style.display='none';
					}
				}
				
				function closeEmailLayer() {
					var a = document.getElementById("wrap").getAttribute("whereEmail");
					if (a=="dock") viewEmailToFriend('off');
					else showTellaFriendLayer('off','tellaFriend');
				
				}
